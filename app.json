{
  "expo": {
    "name": "bloomWellness-app",
    "slug": "bloomWellness-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Bw-icon.png",
    "scheme": "bloomwellnessapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "imageWidth": 200,
      "image": "./assets/images/Bw-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E3B36"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dimitrivld.bloomWellness-app",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.dimitrivld.bloomWellnessapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Bw-icon.png"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "plugins": [
      "expo-apple-authentication",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.YOUR_IOS_CLIENT_ID"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d14c6c3e-fc49-485b-ac5d-8b806f786815"
      }
    }
  }
}
